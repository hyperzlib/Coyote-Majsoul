{
  "name": "coyote-majsoul",
  "version": "1.0.0",
  "author": "Hyperzlib",
  "license": "AGPL-3.0-or-later",
  "bin": "dist-server/index.js",
  "main": "dist-server/index.js",
  "scripts": {
    "build:user": "vite build && node scripts/build-user.js",
    "build:frontend": "cd frontend && npm run build",
    "build:schema": "node cli/build-schema.js",
    "build:server": "npm run build:schema && tsc -p tsconfig.node.json && shx cp -r server/schemas dist-server",
    "build:pkg:assets": "shx mkdir -p build/frontend && shx cp -r frontend/dist build/frontend/dist && shx cp config-example.json5 build && shx cp README.md build && shx cp LICENSE build && shx cp -r build-assets/* build",
    "build:pkg:win": "pkg . -t node18-win-x64 --out-path=build",
    "build:pkg:linux": "pkg . -t node18-linux-x64 --out-path=build",
    "build:pkg:clean": "shx rm -rf build",
    "start": "ts-node server/index.ts"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/koa-cors": "^0.0.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.4",
    "@types/koa__cors": "^5.0.0",
    "@types/tampermonkey": "^5.0.3",
    "pkg": "^5.8.1",
    "rollup": "^3.26.3",
    "shx": "^0.3.4",
    "ts-json-schema-generator": "^2.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^3.0.9"
  },
  "dependencies": {
    "@hyperzlib/node-reactive-config": "^1.0.1",
    "@koa/cors": "^5.0.0",
    "@pondwader/socks5-server": "^1.0.10",
    "ajv": "^8.17.1",
    "deepcopy": "^2.1.0",
    "eventemitter2": "^6.4.7",
    "got": "^11.8.6",
    "json5": "^2.2.3",
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "protobufjs": "^7.2.4",
    "winston": "^3.10.0",
    "yaml": "^2.4.5"
  }
}
